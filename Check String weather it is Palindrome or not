import java.io.*;

class GFG {
    public boolean isPalindrome(String s)
    {
       int i=0;
       int j=s.length()-1;
       while(i<j)
       {
           if(s.charAt(i) != s.charAt(j))
           {
               return false;
           }
           i++;
           j--;
       }
        return true;
    }
   

    
    
	public static void main (String[] args) {
		//System.out.println("GfG!");
		String s1 = "aabaac";
		GFG g = new GFG();
		System.out.println(g.isPalindrome(s1));
	}
}
